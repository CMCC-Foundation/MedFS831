        REAL(R8)   , DYNMEM     ::  YTL_AD(:)  ! DELTA Y OF TRAJECTORY CORRECTION (ADJOINT)
        REAL(R8)   , DYNMEM     ::  XOB(:)     ! OBSERVATION OF THE LAST TRAJECTORY POSITION
        REAL(R8)   , DYNMEM     ::  YOB(:)     ! OBSERVATION OF THE LAST TRAJECTORY POSITION


