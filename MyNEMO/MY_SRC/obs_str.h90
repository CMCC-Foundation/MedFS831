INTEGER, PARAMETER ::                &
      R4 = SELECTED_REAL_KIND( 6, 37),  &  ! REAL*4
#ifdef NO_DOUBLEPREC
      R8 = R4
#else
      R8 = SELECTED_REAL_KIND(12,307)      ! REAL*8
#endif

   INTEGER, PARAMETER :: NoSLASATS = 9
   INTEGER, PARAMETER :: NoSSSSATS = 3
   INTEGER, PARAMETER :: NoSSTSATS = 6
