!AS: PREPROC COMMAND TO TUNE INTEGER PRECISION,
!    WAS INT*8 IN THE ORIGINAL VERSION, WHICH
!    MIGHT CAUSE TYPE CONVERSION ERRORS WHEN
!    USING INT*4 AS INDEXES.

#ifndef USE_INT8
#define PREC_INT i4
#else
#define PREC_INT PREC_INT
#endif


