        REAL(R8)   , DYNMEM     ::  BGERR(:)   ! BACKGROUND-ERROR IN OBS SPACE
        INTEGER(PREC_INT), DYNMEM     ::  MOI(:,:)      ! I INDEX OF THE NEAREST WEST POINT
        INTEGER(PREC_INT), DYNMEM     ::  MOJ(:,:)      ! I INDEX OF THE NEAREST WEST POINT
